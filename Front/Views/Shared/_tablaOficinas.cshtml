@model List<Front.Models.TablaModel>;

<hr />

<div class="table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th class="text-center">Letra</th>
                <th class="text-center">Veces</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.Letra)
                    </td>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.Conteo)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>